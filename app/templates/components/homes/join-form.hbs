{{log this}}
<div class="join-home-wrapper">
  <div class="search-form-wrapper">
    <div class="ui search join-search-form">
      <div class="ui segment home-results form">
        <div class="ui icon fluid input">
          <i {{action 'findHomes'}} class="search link icon"></i>
          <input class="prompt" type="text" name="query" value={{query}} autocomplete="on" placeholder="Search..." />
        </div>
        {{#if searchResults}}
          <div class="result results-labels">
            <div class="info">
              <div class="title">
                Name
              </div>
              <div class="zip">
                Zip
              </div>
            </div>
          </div>
          {{#each searchResults as |result|}}
            <div class={{if result.selected 'selected result' 'result'}} {{action 'homeSelected' result}}>
              <div class="info">
                <div class="title">
                  {{result.name}}
                </div>
                <div class="zip">
                  {{result.zip}}
                </div>
              </div>
            </div>
          {{/each}}
          <div class="ui bottom attached button basic all">
            View all results
          </div>
        {{else}}
          <h1 class="no-results">No results</h1>
        {{/if}}
      </div>
      <div class="ui form segment join-password">
        <div class="field">
          <label>
            Password
          </label>
          {{input type="password" name="password" placeholder="Home password" value=selectedHome.password disabled=noSelectedHome}}
        </div>
        <div class="ui green submit join button">
          <span class="submit-text">
            Join
          </span>
        </div>
        <div class="ui error message">
        </div>
      </div>
    </div>
  </div>
</div>
